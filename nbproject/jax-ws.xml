<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="CurrencyConverter">
      <implementation-class>WebServices.CurrencyConverter</implementation-class>
    </service>
    <service name="MoneyTransfer">
      <implementation-class>WebServices.MoneyTransfer</implementation-class>
    </service>
  </services>
  <clients>
    <client name="CurrencyConverter">
      <wsdl-url>http://localhost:8080/Coursework2/CurrencyConverter?wsdl</wsdl-url>
      <local-wsdl-file>localhost_8080/Coursework2/CurrencyConverter.wsdl</local-wsdl-file>
      <package-name forceReplace="true">Services</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8080/Coursework2/CurrencyConverter?wsdl</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>package</wsimport-option-name>
          <wsimport-option-value>Services</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
    <client name="MoneyTransfer">
      <wsdl-url>http://localhost:8080/WebServicesCurrencyExchange-master/MoneyTransfer?wsdl</wsdl-url>
      <local-wsdl-file>localhost_8080/WebServicesCurrencyExchange-master/MoneyTransfer.wsdl</local-wsdl-file>
      <package-name forceReplace="true">WebServices_client</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
